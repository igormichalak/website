{{define "title"}}Igor Michalak{{end}}

{{define "content"}}
<h2>Featured</h2>
<ul class="writing-list">
{{range .AllWritings}}
	<li>
		<a href="/{{.Slug}}" class="writing-title">
			{{.Title}}
		</a>
		<span class="writing-details">
			<span class="font-mono">{{.PublishedAt.Format "2006-01-02"}}</span>
		</span>
	</li>
{{end}}
</ul>
{{end}}
